pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
local t=0

function _update()
	t+=0.01
 for y=0,15 do
  local sy=1.5*sin(t+y/50)
  for x=0,15 do
   sset(24+x,y,sget(
   	8+band((x+sy),0xf),
   	band((y+1.5*sin(t+x/50)),0xf)))
  end
 end 
end

function _draw()
	cls()

	map(0,0,0,0)  
	
	map(16,8,0,34,16,3)
	map(16,2,0,23,16,3)
	
 print(stat(1),2,110,2)
end

