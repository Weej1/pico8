pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
--[[
for x=0,63 do
	for y=0,63 do
		local s0=sget(x,y)
		local s1=sget(x+64,y)
		sset(x,y+64,s0+shl(s1,2))
	end
end
cstore()
]]

local colors={0,1,0,14}
function set_layer(top)
	pal()
 for i=0,15 do
  local id=(top and i%4 or flr(i/4))
  if id==0 then
   palt(i,true)
  else
   pal(i,colors[id+1])
  end
 end
end

local layer = false

function _update()
 -- toggle layer on button press
 if btnp(4) then
  layer=not layer
 end
end
function _draw()
	cls(9)
	pal()
	set_layer(not layer)
	spr(192,0,32,8,4)
	set_layer(layer)
	spr(128,0,64,8,8)
end

		
