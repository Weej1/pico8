pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
local qmap=""

local colors={3,4,9}
cls(11)
local i=1
local read_nibles=function(n)
	local v=tonum("0x"..sub(qmap,i,i+n-1))
	i+=n
	return v
end
local n=read_nibles(4)
i=1
while i<=n do
	-- coordinates
	local idx=read_nibles(4)
	local x,y=idx%128,flr(idx/128)
	-- count
	local count=read_nibles(2)
	--print(qhex.."=>"..count)
	for j=0,count-1 do
 	local q=read_nibles(1)
 	local c=read_nibles(1)
		pset(x+j,y,colors[band(c,3)])
		--print("color:"..c) 		
 end
 flip()
end

