version 16
__lua__
local data=""
local qmap={}
local hmap={}
local track={}

local colors={3,4,9}
function _init()
	cls(11)
	local i=1
	local read_nibles=function(n)
		local v=tonum("0x"..sub(data,i,i+n-1))
		i+=n
		return v
	end
	local unpack_rle=function(decode)
		local read=true
		while read do
			-- peek data
			local k,q=read_nibles(2)
			-- rle or value?
			if band(0x80,k)>0 then
			-- run length
				k,q=band(0x7f,k),read_nibles(2)
			else
				k,q=1,k
			end
			for j=1,k do
				read=decode(q)
			end
		end
	end
	local i=0
	unpack_rle(function(q)
		qmap[i]=q
		i+=1
		return i<0x4000
	end)
	i=0
	local tmp_hmap={}
	unpack_rle(function(v)
		tmp_hmap[i],tmp_hmap[i+1]=shr(band(0xf0,v),4),band(0xf,v)
		i+=2
		return i<0x1000
	end)
 	-- linear lerp to 128x128
	for j=0,62 do
		for i=0,62 do
			local idx=i+64*j
			local h0,h1,h3=tmp_hmap[idx],tmp_hmap[idx+64],tmp_hmap[idx+1]
			idx=2*i+256*j
			hmap[idx]=h0
			hmap[idx+1]=(h0+h3)/2
			hmap[idx+128]=(h0+h1)/2
			hmap[idx+129]=(h1+h3)/2
		end
	end

	for k=1,read_nibles(2) do
		add(track,{read_nibles(2),read_nibles(2)})
	end
end

local x,y=0,0
function _update()
	local dx,dy=0,0
	if(btn(0)) dx=1
	if(btn(1)) dx=-1
	if(btn(2)) dy=1
	if(btn(3)) dy=-1
	
	x+=dx
	y+=dy
end

function _draw()
 cls()
 --[[
 local jmin=band(flr(y),0x7f)
	for j=1,16 do
	 local imin=band(flr(x),0x7f)
	 local idx=imin+128*jmin
		for i=1,16 do
			local q=qmap[idx]
			local hi,lo=shr(band(0b00111000,q),3),band(0b111,q)
			hi,lo=colors[hi+1],colors[lo+1]
			q=band(0x40,q)
			pal(1,hi)
			pal(14,lo)
			spr(q>0 and 2 or 1,i*8,j*8)
			
			--local h=hmap[idx]
			--rectfill(i*8,j*8,i*8+7,j*8+7,h)
		 idx+=1
		end
		jmin+=1
 end
 pal()
 ]]

 for j=0,127 do
 	for i=0,127 do
 		local idx=i+128*j
 		local h=hmap[idx]
 		pset(i,j,sget(24+h))
 	end
 end
 
 -- print(stat(1),2,2,7)
end
