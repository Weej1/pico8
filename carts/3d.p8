pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
cam={}
ramp={12,1}
grass={3,11}
buildings={}
flr_n=8
flr_h=2
flr_w=16
flr_z=-64
focal=8
hh=128/2
hw=128/2
function clamp(a,mi,ma)
	if (a<mi) return mi
	if (a>ma) return ma
	return a
end
function _update()
	if (btn(0)) cam.x-=1
 if (btn(1)) cam.x+=1 	
 cam.x=clamp(cam.x,-32,32)
 if (btn(2)) cam.y-=1
 if (btn(3)) cam.y+=1
 cam.y=clamp(cam.y,-128,128)
 if (btn(4)) cam.z-=0.5			
 if (btn(5)) cam.z+=0.5
 cam.z=clamp(cam.z,-64,0)
end
function draw_building(b,i)
	local x,y
	x=b.x-cam.x
	y=b.y-cam.y
	local xe,ye,ze,z,c
	z=i*flr_h+flr_z-cam.z
	xe=hw-focal*x/z
	ye=hh-focal*y/z
	ze=focal*flr_w/z
	c=ramp[i%2+1]
	rectfill(xe-ze,ye-ze,xe+ze,ye+ze,c)
end
function draw_buildopt()
	for i=0,flr_n-1 do
		local ze,z,c,w
		z=i*flr_h+flr_z-cam.z
		w=1/z
		ze=focal*flr_w*w
		c=ramp[i%2+1]
		foreach(buildings,
			function(b)
				local x,y,xe,ye
				x=b.x-cam.x
				y=b.y-cam.y
				xe=hw-focal*x*w
				ye=hh-focal*y*w				
				if(i%2==1) then
					sspr(40,0,32,32,xe-ze,ye-ze,2*ze,2*ze)
				else
			 	rectfill(xe-ze,ye-ze,xe+ze,ye+ze,5)
			 end
			end)
	end
end

function draw_floor()
	local ze,z,k,n,r,ye,w,dy,c
	z=flr_z-cam.z
	w=1/z
	ze=-2*focal*flr_w*w
 k=flr(128/ze+0.5)
	--r=ze%2
 --if(r~=0) ze+=2-r
 dy=focal*cam.y*w
 ye=64+(dy%(2*ze))
 n=flr(ye/ze+0.5)
 for i=1,n do
 	c=grass[i%2+1]
	 rectfill(0,ye-i*ze,127,ye-i*ze+ze,c) 	
 end
 for i=0,k do
 	c=grass[i%2+1]
	 rectfill(0,ye,127,ye+ze,c)
 	ye+=ze
	end
end

function _draw()
	-- cls(grass[1])
	local xe,ye
	xe=hw-0-cam.x
	ye=hh-0-cam.y
	line(xe,ye,xe,ye-16,11)
	line(xe,ye,xe+16,ye,8)
	
	--for i=0,flr_n-1 do
	--	foreach(buildings,function(b) draw_building(b,i) end)
	--end
	draw_floor()
	draw_buildopt()
	palt(0,false)
	palt(3,true)
	spr(64,64-8,64+16,2,3)
	palt()
	
	print("cpu "..stat(1),0,0,12)
end
function _init()
cam.x=0
cam.y=0
cam.z=0
for i=1,5 do
	local b={}
	b.x=rnd(2*96)-96
	b.y=rnd(2*96)-96
	buildings[i]=b 
end
end
